{
  "manifest_version": 3,
  "name": "Customable Chrome Homepage",
  "version": "1.0",
  "description": "A customable homepage extension for Chrome",
  "action": {
    "default_icon": "/icons/16.svg"
  },
  "icons": {
      "16": "/icons/16.svg",
      "48": "/icons/48.svg",
      "128": "/icons/128.svg"
  },
  "permissions": [],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
