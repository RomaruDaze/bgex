{
    "manifest_version": 3,
    "name": "My React Chrome Extension",
    "version": "1.0",
    "description": "A React app as a Chrome extension",
    "action": {
      "default_popup": "index.html",
      "default_icon": "16.svg"
    },
    "icons": {
        "16": "16.svg",
        "48": "48.svg",
        "128": "128.svg"
    },
    "permissions": [],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    }
}
